name: Telecom_Cookie
desc: 开启脚本以及mitm等开关，获取覆写后关闭

http:
  mitm:
    - e.dlife.cn
  script:
    - match: ^https?:\/\/e\.dlife\.cn
      name: Telecom_CK
      type: request
      require-body: true
      timeout: 120


script-providers:
  Telecom_CK:
    url: https://raw.githubusercontent.com/QGCliveDavis/Module/main/Ck1000.js
    interval: 86400
