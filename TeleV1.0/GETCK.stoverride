name: 电信免流Cookie获取
desc: 开启脚本以及mitm等开关，配合电信免流使用

http:
  mitm:
    - e.dlife.cn
  script:
    - match: ^https?:\/\/e\.dlife\.cn
      name: Telecom_CK
      type: request
      require-body: true
      timeout: 120


script-providers:
  Telecom_CK:
    url: https://raw.githubusercontent.com/QGCliveDavis/Module/main/TeleV1.0/Ck1000.js
    interval: 86400
